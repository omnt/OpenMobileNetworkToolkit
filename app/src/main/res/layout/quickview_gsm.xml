<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:id="@+id/quickview_gsm">

    <androidx.cardview.widget.CardView
        android:layout_height="wrap_content"
        android:layout_width="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:tag="row_1">
                <!-- Cell Type -->
                <androidx.cardview.widget.CardView
                    style="@style/quickViewChildCardViewStyle">
                    <LinearLayout
                        style="@style/quickViewChildCardViewLL">
                        <com.google.android.material.textview.MaterialTextView
                            style="@style/quickViewTextViewTitle"
                            android:text="@string/cellType">
                        </com.google.android.material.textview.MaterialTextView>
                        <com.google.android.material.textview.MaterialTextView
                            style="@style/quickViewTextViewValue"
                            android:id="@+id/quickview_gsm_cellType"
                            android:text="@string/na">
                        </com.google.android.material.textview.MaterialTextView>
                    </LinearLayout>
                </androidx.cardview.widget.CardView>
                <!--PLMN-->
                <androidx.cardview.widget.CardView
                    style="@style/quickViewChildCardViewStyle">
                    <LinearLayout
                        style="@style/quickViewChildCardViewLL">
                        <com.google.android.material.textview.MaterialTextView
                            style="@style/quickViewTextViewTitle"
                            android:text="@string/plmn">
                        </com.google.android.material.textview.MaterialTextView>
                        <com.google.android.material.textview.MaterialTextView
                            style="@style/quickViewTextViewValue"
                            android:id="@+id/quickview_gsm_plmn"
                            android:text="@string/na">
                        </com.google.android.material.textview.MaterialTextView>
                    </LinearLayout>
                </androidx.cardview.widget.CardView>
                <!-- CI -->
                <androidx.cardview.widget.CardView
                    style="@style/quickViewChildCardViewStyle">
                    <LinearLayout
                        style="@style/quickViewChildCardViewLL">
                        <com.google.android.material.textview.MaterialTextView
                            style="@style/quickViewTextViewTitle"
                            android:text="@string/ci">
                        </com.google.android.material.textview.MaterialTextView>
                        <com.google.android.material.textview.MaterialTextView
                            style="@style/quickViewTextViewValue"
                            android:id="@+id/quickview_gsm_ci"
                            android:text="@string/na">
                        </com.google.android.material.textview.MaterialTextView>
                    </LinearLayout>
                </androidx.cardview.widget.CardView>
                <!-- BSIC -->
                <androidx.cardview.widget.CardView
                    style="@style/quickViewChildCardViewStyle">
                    <LinearLayout
                        style="@style/quickViewChildCardViewLL">
                        <com.google.android.material.textview.MaterialTextView
                            style="@style/quickViewTextViewTitle"
                            android:text="@string/bsic">
                        </com.google.android.material.textview.MaterialTextView>
                        <com.google.android.material.textview.MaterialTextView
                            style="@style/quickViewTextViewValue"
                            android:id="@+id/quickview_gsm_bsic"
                            android:text="@string/na">
                        </com.google.android.material.textview.MaterialTextView>
                    </LinearLayout>
                </androidx.cardview.widget.CardView>
                <!-- LAC -->
                <androidx.cardview.widget.CardView
                    style="@style/quickViewChildCardViewStyle">
                    <LinearLayout
                        style="@style/quickViewChildCardViewLL">
                        <com.google.android.material.textview.MaterialTextView
                            style="@style/quickViewTextViewTitle"
                            android:text="@string/lac">
                        </com.google.android.material.textview.MaterialTextView>
                        <com.google.android.material.textview.MaterialTextView
                            style="@style/quickViewTextViewValue"
                            android:id="@+id/quickview_gsm_lac"
                            android:text="@string/na">
                        </com.google.android.material.textview.MaterialTextView>
                    </LinearLayout>
                </androidx.cardview.widget.CardView>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:tag="row_2">
                <!-- RSSI -->
                <androidx.cardview.widget.CardView
                    style="@style/quickViewChildCardViewStyle">
                    <LinearLayout
                        style="@style/quickViewChildCardViewLL">
                        <com.google.android.material.textview.MaterialTextView
                            style="@style/quickViewTextViewTitle"
                            android:text="@string/rssi">
                        </com.google.android.material.textview.MaterialTextView>
                        <com.google.android.material.textview.MaterialTextView
                            style="@style/quickViewTextViewValue"
                            android:id="@+id/quickview_gsm_rssi"
                            android:text="@string/na">
                        </com.google.android.material.textview.MaterialTextView>
                    </LinearLayout>
                </androidx.cardview.widget.CardView>
                <!--BER-->
                <androidx.cardview.widget.CardView
                    style="@style/quickViewChildCardViewStyle">
                    <LinearLayout
                        style="@style/quickViewChildCardViewLL">
                        <com.google.android.material.textview.MaterialTextView
                            style="@style/quickViewTextViewTitle"
                            android:text="@string/bitErrorRate_short">
                        </com.google.android.material.textview.MaterialTextView>
                        <com.google.android.material.textview.MaterialTextView
                            style="@style/quickViewTextViewValue"
                            android:id="@+id/quickview_gsm_ber"
                            android:text="@string/na">
                        </com.google.android.material.textview.MaterialTextView>
                    </LinearLayout>
                </androidx.cardview.widget.CardView>
        </LinearLayout>
        </LinearLayout>

    </androidx.cardview.widget.CardView>


</LinearLayout>
